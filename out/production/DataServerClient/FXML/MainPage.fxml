<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller"
            prefHeight="200" prefWidth="300"
            fx:id="mainBorderPane">
    <center>
        <VBox spacing="5" alignment="CENTER">
            <Button fx:id="registerButton" text = "Register" onAction="#toRegisterPage" VBox.vgrow="ALWAYS" prefWidth="150"/>
            <Button fx:id="loginButton" text = "Log in" onAction="#toLoginPage" VBox.vgrow="ALWAYS" prefWidth="150"/>
        </VBox>
    </center>
</BorderPane>
